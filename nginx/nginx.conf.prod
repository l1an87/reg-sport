server {
  listen 80;

  server_name reg.nashaliga.ru;
  client_max_body_size 20M;


  location / {
    proxy_pass http://ui:3000;
  }

  location /api {
    rewrite ^/api/(.*) /$1 break;
    proxy_pass http://api:3001;
  }
}
server {
  listen 80;

  server_name  zima.sistemasport.ru;
  client_max_body_size 20M;


  location / {
    proxy_pass http://landing:3000;
  }
}
